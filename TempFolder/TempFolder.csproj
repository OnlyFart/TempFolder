<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1;netcoreapp1.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>TempFolder</Title>
        <Authors>OnlyFart</Authors>
        <Description>Helper for creating temporary folders that are automatically deleted after use</Description>
        <PackageProjectUrl>https://github.com/OnlyFart/TempFolder</PackageProjectUrl>
        <RepositoryUrl>https://github.com/OnlyFart/TempFolder</RepositoryUrl>
        <PackageTags>folder tempfolder</PackageTags>
        <PackageLicenseUrl>https://licenses.nuget.org/Unlicense</PackageLicenseUrl>
    </PropertyGroup>

</Project>
